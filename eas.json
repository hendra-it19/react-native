{
  "cli": {
    "version": ">= 16.3.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.14.0",
      "env": {
        "API_URL": "https://development-example.com/api"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "22.14.0",
      "env": {
        "API_URL": "https://production-example.com/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
